<%= form_for(@admin) do |f| %>
  <% if @admin.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@admin.errors.count, "error") %> prohibited this admin from being saved:</h2>

      <ul>
      <% @admin.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :fname %><br>
    <%= f.text_field :fname %>
  </div>
  <div class="field">
    <%= f.label :lname %><br>
    <%= f.text_field :lname %>
  </div>
  <div class="field">
    <%= f.label :phoneNo %><br>
    <%= f.number_field :phoneNo %>
  </div>
  <div class="field">
    <%= f.hidden_field :user_id, :value=>"#{session[:user_id]}"%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
